{
  "name": "hostr",
  "description": "Hostr - simple sharing",
  "repository": "https://github.com/kudos/hostr-web",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": "^10.1.0"
  },
  "scripts": {
    "build": "npm run build-js && npm run build-sass",
    "build-js": "webpack --progress -p -c webpack.config.js",
    "build-sass": "node-sass --include-path ./node_modules/ -r -o web/public/styles/ web/public/styles/",
    "cover": "istanbul cover _mocha -- -r babel-register test/**/*.spec.js",
    "heroku-postbuild": "npm run build",
    "init": "babel-node -e \"require('./lib/storage').default();\"",
    "initdb": "node -r babel-register test/initdb.js",
    "lint": "eslint .",
    "start": "node -r babel-register app.js",
    "test": "npm run test-seed && mocha -r babel-register test/**/*.spec.js",
    "test-seed": "babel-node test/fixtures/user.js",
    "watch": "concurrently -k -n watch-js,watch-server \"npm run watch-js\" \"npm run watch-server\"",
    "watch-js": "webpack --mode=development --progress -c webpack.config.js",
    "watch-server": "nodemon -r babel-register -i web/public",
    "watch-sass": "node-sass --include-path ./node_modules/ -w -r -o web/public/styles/ web/public/styles/"
  },
  "dependencies": {
    "angular": "^1.7.3",
    "angular-reconnecting-websocket": "https://github.com/adieu/angular-reconnecting-websocket#0.1.1",
    "angular-resource": "^1.7.3",
    "angular-route": "^1.7.3",
    "angular-strap": "^2.3.8",
    "async-busboy": "^0.7.0",
    "aws-sdk": "^2.291.0",
    "babel": "^6.5.2",
    "babel-cli": "^6.10.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-register": "^6.9.0",
    "basic-auth": "~2.0.0",
    "bootstrap-sass": "^3.3.6",
    "busboy": "^0.2.14",
    "co": "~4.6.0",
    "co-redis": "^2.1.0",
    "co-views": "~2.1.0",
    "copy-webpack-plugin": "^4.5.2",
    "debug": "~3.1.0",
    "dropzone": "~5.5.1",
    "ejs": "^2.6.1",
    "form-data": "^2.3.2",
    "http-errors": "^1.7.0",
    "image-size": "^0.6.3",
    "image-type": "^3.0.0",
    "jimp": "0.3.3",
    "jquery": "^3.3.1",
    "kcors": "^2.2.2",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-compress": "~3.0.0",
    "koa-csrf": "^3.0.6",
    "koa-error": "^3.2.0",
    "koa-favicon": "~2.0.1",
    "koa-generic-session": "^2.0.1",
    "koa-helmet": "^4.0.0",
    "koa-logger": "~3.2.0",
    "koa-redis": "^3.1.2",
    "koa-router": "^7.4.0",
    "koa-session": "^5.8.2",
    "koa-static": "^5.0.0",
    "koa-statsd": "~0.0.2",
    "koa-views": "^6.1.4",
    "koa-websocket": "^5.0.1",
    "kue": "^0.11.6",
    "mime-types": "^2.1.19",
    "moment": "^2.22.1",
    "mz": "^2.7.0",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.9.3",
    "node-uuid": "^1.4.8",
    "passwords": "^1.3.1",
    "pg": "^7.4.3",
    "raven": "^2.6.3",
    "redis": "^2.8.0",
    "@sendgrid/mail": "^6.3.1",
    "sequelize": "^4.38.0",
    "smooth-scroll": "https://github.com/cferdinandi/smooth-scroll#5.3.7",
    "statsy": "~0.2.0",
    "stripe": "^6.7.0",
    "swig": "~1.4.2",
    "validate-ip": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "zeroclipboard": "^2.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "concurrently": "^3.6.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "istanbul": "^0.4.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.3",
    "supertest": "^3.1.0",
    "tmp": "0.0.33"
  }
}
